{
  "name": "@swan/partner-mobile-server",
  "description": "Swan mobile server",
  "license": "MIT",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": ">=20.9.0",
    "pnpm": "9.9.0"
  },
  "scripts": {
    "build": "pnpm clean && tsc && rm dist/tsconfig.tsbuildinfo",
    "clean": "rm -rf dist",
    "dev": "tsx watch --clear-screen=false src/app",
    "start": "node dist/server",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/autoload": "6.0.2",
    "@fastify/cookie": "11.0.1",
    "@fastify/helmet": "12.0.1",
    "@fastify/reply-from": "11.0.1",
    "@fastify/sensible": "6.0.1",
    "@hapi/iron": "7.0.1",
    "axios": "1.7.7",
    "close-with-grace": "2.1.0",
    "dotenv": "16.4.5",
    "fastify": "5.1.0",
    "fastify-healthcheck": "5.0.0",
    "fastify-plugin": "5.0.1",
    "ts-pattern": "5.5.0",
    "valienv": "0.7.0"
  },
  "devDependencies": {
    "@types/node": "20.17.6",
    "pino-pretty": "11.3.0",
    "tsx": "4.19.2",
    "type-fest": "4.26.1",
    "typescript": "5.6.3"
  }
}
