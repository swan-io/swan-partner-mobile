    "fastify-healthcheck": "4.4.0",
    "fastify-plugin": "4.5.1",
    "ts-pattern": "5.0.8",
    "valienv": "0.6.0"
  },
  "devDependencies": {
    "@types/node": "18.19.22",
    "pino-pretty": "10.3.1",
