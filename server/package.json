{
  "name": "@swan/partner-mobile-server",
  "description": "Swan mobile server",
  "license": "MIT",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "build": "yarn clean && tsc && rm dist/tsconfig.tsbuildinfo",
    "clean": "rm -rf dist",
    "dev": "tsx watch --clear-screen=false src/app",
    "start": "node dist/server",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/autoload": "5.8.0",
    "@fastify/cookie": "9.1.0",
    "@fastify/helmet": "11.1.1",
    "@fastify/reply-from": "9.4.0",
    "@fastify/sensible": "5.5.0",
    "@hapi/iron": "7.0.1",
    "axios": "1.6.1",
    "close-with-grace": "1.2.0",
    "dotenv": "16.3.1",
    "fastify": "4.24.3",
    "fastify-healthcheck": "4.4.0",
    "fastify-plugin": "4.5.1",
    "ts-pattern": "5.0.5",
    "valienv": "0.5.0"
  },
  "devDependencies": {
    "@types/node": "18.18.6",
    "pino-pretty": "10.2.3",
    "tsx": "4.1.2",
    "type-fest": "4.7.1",
    "typescript": "5.2.2"
  }
}
